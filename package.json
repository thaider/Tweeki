        "laravel-mix": "^5.0.9",
        "popper.js": "^1.16",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.29.0",
        "sass-loader": "^7.1.0",
        "vue-template-compiler": "^2.6.5"
    },
